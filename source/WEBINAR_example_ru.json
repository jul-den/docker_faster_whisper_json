{
  "type": "video_transcription",
  "name": "webinar_example",
  "description": "Полная расшифровка видео WEBINAR.MOV",
  "steps": [
    {
      "action": "extract_audio",
      "input": "WEBINAR.MOV",
      "output": "full_audio_WEBINAR.wav",
      "params": {
        "ar": 16000,
        "ac": 1
      }
    },
    {
      "action": "transcribe",
      "input": "full_audio_WEBINAR.wav",
      "output": "full_transcript_WEBINAR.txt",
      "language": "ru",
      "beam_size": 5,
      "combine": true
    }
  ]
}